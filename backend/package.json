{
  "name": "jengu-backend",
  "version": "1.0.0",
  "description": "Secure backend API server for Jengu Dynamic Pricing Platform",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf -f dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "tsx test/api-integration.test.ts",
    "test:generate-data": "tsx test/e2e-test-data.ts"
  },
  "keywords": [
    "api",
    "proxy",
    "pricing",
    "hotel"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^10.20.0",
    "@sentry/profiling-node": "^10.20.0",
    "@supabase/supabase-js": "^2.75.0",
    "axios": "^1.6.8",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "multer": "^2.0.2",
    "pino": "^10.0.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.8.0",
    "@types/supertest": "^6.0.3",
    "@vitest/ui": "^3.2.4",
    "supertest": "^7.1.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
